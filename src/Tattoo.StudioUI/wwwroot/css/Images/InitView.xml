<svg width="800" height="300" viewBox="0 0 800 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <style type="text/css">
      .text-outline {
        fill: none;
        stroke: black;
        stroke-width: 1;
        stroke-dasharray: 700;
        stroke-dashoffset: 700;
        animation: draw 12s forwards;
        font-weight: 200;
      }

      .text-fill {
        fill: #3e3e3e;
        opacity: 0;
        animation: fill 12s forwards 1s;
      }

      @keyframes draw {
        to {
          stroke-dashoffset: 0;
        }
      }

      @keyframes fill {
        to {
          opacity: 1;
        }
      }
    </style>
  </defs>

  <!-- Text -->
  <g>
    <text x="400" y="100" font-size="100" font-family="emoji" class="text-outline" text-anchor="middle">ink</text>
    <text x="400" y="200" font-size="100" font-family="emoji" class="text-outline" text-anchor="middle">VibesTattoo</text>
    <text x="400" y="100" font-size="100" font-family="emoji" class="text-fill" text-anchor="middle">ink</text>
    <text x="400" y="200" font-size="100" font-family="emoji" class="text-fill" text-anchor="middle">VibesTattoo</text>
  </g>
</svg>
